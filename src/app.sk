@import
namespace console {
    const log dynamic
}

namespace Controller {
    const cellSize = 1
}

class HTMLWindow {
    var onload dynamic
    var onresize dynamic
}

@entry
def main {
    window.onload = => {
        const canvas = document.getElementById("gol-canvas") as HTMLCanvasElement
        const controller = Controller.new(canvas)
        controller.draw
        controller.start
    }
}

